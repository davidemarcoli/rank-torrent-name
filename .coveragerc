[run]
branch = True
source = RTN

[report]
omit =
    */tests/*
    */__init__.py

# Show missing lines in report output
show_missing = True

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
# Directory where HTML reports will be saved
directory = htmlcov